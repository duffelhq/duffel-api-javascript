{
  "name": "@duffel/duffel-api",
  "version": "0.0.3",
  "description": "Javascript client library for the Duffel API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/duffelhq/duffel-api-javascript.git"
  },
  "homepage": "https://duffel.com",
  "bugs": {
    "url": "https://github.com/duffelhq/duffel-api-javascript/issues"
  },
  "files": [
    "dist"
  ],
  "author": "Duffel Technology Limited <help@duffel.com> (http://duffel.com)",
  "license": "MIT",
  "scripts": {
    "generate:method": "node ./scripts/createMethod.mjs",
    "build": "tsc",
    "start": "node ./dist/index.js",
    "dev": "ts-node ./src/index.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest",
    "pre-push-check": "yarn lint && yarn test && yarn build --noEmit ",
    "prepare": "husky install",
    "example": "ts-node ./examples/example.ts"
  },
  "dependencies": {
    "@types/node": "^15.0.2",
    "dotenv": "^9.0.1",
    "isomorphic-unfetch": "^3.1.0",
    "lodash": "^4.17.21",
    "nock": "^13.0.11",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.168",
    "@types/node-fetch": "^2.5.10",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "babel-jest": "^26.6.3",
    "colors": "^1.4.0",
    "eslint": "^7.26.0",
    "eslint-plugin-spellcheck": "^0.0.17",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1"
  },
  "engines": {
    "node": "~14.*"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
