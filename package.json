{
  "name": "@duffel/api",
  "version": "1.3.0",
  "description": "Javascript client library for the Duffel API",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/types/index.d.ts",
  "homepage": "https://duffel.com",
  "keywords": [
    "duffel"
  ],
  "files": [
    "dist"
  ],
  "author": "Duffel Technology Limited <help@duffel.com> (http://duffel.com)",
  "license": "MIT",
  "scripts": {
    "generate:operation": "node ./scripts/createOperation.mjs",
    "build": "rm -rf dist && rollup -c",
    "build:test": "rm -rf dist && tsc --noEmit && rollup -c",
    "start": "node ./dist/index.js",
    "dev": "ts-node ./src/index.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest",
    "pre-push-check": "yarn lint && yarn test && yarn build:test",
    "prepare": "husky install",
    "example": "ts-node ./examples/example.ts",
    "get-version": "echo $npm_package_version",
    "release": "npx semantic-release",
    "commit": "git-cz",
    "commitlint": "commitlint"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "@types/node": "^16.0.0",
    "isomorphic-unfetch": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-angular": "^13.1.0",
    "@commitlint/cz-commitlint": "^13.1.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-multi-entry": "^4.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/jest": "^27.0.1",
    "@types/node-fetch": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "colors": "^1.4.0",
    "commitizen": "^4.2.4",
    "dotenv": "^10.0.0",
    "eslint": "^7.26.0",
    "eslint-plugin-spellcheck": "^0.0.19",
    "husky": "^7.0.0",
    "jest": "^26.6.3",
    "nock": "^13.1.0",
    "rollup": "^2.51.2",
    "rollup-plugin-dts": "^3.0.2",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "engines": {
    "node": ">=12.*"
  },
  "plugins": [
    [
      "@semantic-release/commit-analyzer"
    ],
    [
      "@semantic-release/release-notes-generator"
    ],
    "@semantic-release/npm",
    "@semantic-release/github"
  ],
  "release": {
    "branches": [
      "main"
    ]
  }
}
