{
  "name": "@duffel/api",
  "version": "2.5.3",
  "description": "Javascript client library for the Duffel API",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/typings.d.ts",
  "homepage": "https://duffel.com",
  "keywords": [
    "duffel"
  ],
  "files": [
    "dist"
  ],
  "author": "Duffel Technology Limited <help@duffel.com> (http://duffel.com)",
  "license": "MIT",
  "scripts": {
    "generate:operation": "node ./scripts/createOperation.mjs",
    "build": "rm -rf dist && rollup -c && yarn remove-declaration-files",
    "remove-declaration-files": "rm -rf dist/types dist/Client.d.ts dist/index.d.ts dist/Resource.d.ts",
    "build:test": "rm -rf dist && tsc --noEmit && rollup -c",
    "start": "node ./dist/index.js",
    "dev": "ts-node ./src/index.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --runInBand",
    "pre-commit-check": "yarn lint-staged && yarn test && yarn build:test",
    "prepare": "husky install",
    "example": "ts-node ./examples/example.ts",
    "get-version": "echo $npm_package_version",
    "release": "npx semantic-release",
    "commit": "git-cz",
    "commitlint": "commitlint",
    "prettier": "prettier --write --loglevel=warn .",
    "prettier-check": "prettier --check ."
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint",
      "prettier --write"
    ],
    "*.{js,jsx,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "resolutions": {
    "ansi-regex": "^5.0.1",
    "set-value": "^4.0.1",
    "minimist": "1.2.8",
    "semver": "^7.5.3",
    "word-wrap": "npm:@aashutoshrathi/word-wrap"
  },
  "dependencies": {
    "@types/node": "^17.0.21",
    "@types/node-fetch": "^2.6.2",
    "node-fetch": "2.6.11"
  },
  "devDependencies": {
    "@babel/core": "7.23.0",
    "@babel/preset-env": "7.22.20",
    "@babel/preset-typescript": "7.23.0",
    "@commitlint/cli": "17.6.6",
    "@commitlint/config-angular": "17.6.6",
    "@commitlint/cz-commitlint": "17.5.0",
    "@rollup/plugin-commonjs": "24.1.0",
    "@rollup/plugin-node-resolve": "15.1.0",
    "@types/jest": "29.0.3",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "colors": "1.4.0",
    "commitizen": "4.3.0",
    "dotenv": "16.3.1",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-spellcheck": "0.0.20",
    "husky": "8.0.3",
    "jest": "29.0.3",
    "lint-staged": "13.2.2",
    "nock": "13.3.3",
    "prettier": "2.8.8",
    "rollup": "3.21.8",
    "rollup-plugin-dts-bundle": "^1.0.0",
    "rollup-plugin-inject-process-env": "1.3.1",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.33.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">=18.*"
  },
  "release": {
    "plugins": [
      [
        "@semantic-release/commit-analyzer"
      ],
      [
        "@semantic-release/release-notes-generator"
      ],
      "@semantic-release/npm",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ],
    "branches": [
      "main"
    ]
  }
}
